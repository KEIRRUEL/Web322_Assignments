<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <% if (error) { %>
    <div class="alert alert-danger"><%= error %></div>
    <% } %>
    <h1>Categories</h1>
    <ul>
      <% categories.forEach(function(category) { %>
      <li><%= category.name %></li>
      <% }); %>
    </ul>
  </body>
</html>
